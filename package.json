    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}